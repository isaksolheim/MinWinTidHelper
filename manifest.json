{
  "manifest_version": 2,
  "name": "MinWinTidHelper",
  "version": "0.3",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "music/elevator.mp3"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.no/Vedlikehold"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "icons": { "48": "owl-head.png" }
}
